@model IEnumerable<ProjetTexcel_WEB.tblPlatform>

@{
    ViewBag.Title = "Platformes";
}

<h2>@ViewBag.Title</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.nom)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.config)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.typePlatform)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.tag)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.tblSystemExploitation.nom)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.nom)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.config)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.typePlatform)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.tag)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.tblSystemExploitation.nom)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.idPlatform }) |
            @Html.ActionLink("Details", "Details", new { id=item.idPlatform }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.idPlatform })
        </td>
    </tr>
}

</table>
